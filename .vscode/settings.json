{
  "files.encoding": "utf8",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.eol": "\n",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "biomejs.biome",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome",
    "files.encoding": "utf8"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome",
    "files.encoding": "utf8"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome",
    "files.encoding": "utf8"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  }
}
